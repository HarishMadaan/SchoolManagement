@model School.Shared.CustomModels.FeeMasterCustomModel

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}

<div class="page-title">
    <div>
        <h1><i class="fa fa-edit"></i> Fee Master</h1>
    </div>
    <div>
        <ul class="breadcrumb">
            <li><a href="@Url.Action("Index", "Dashboard")"><i class="fa fa-home fa-lg"></i></a></li>
            <li>@Html.ActionLink("FeeMaster", "Index")</li>
        </ul>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="row">
                <div class="col-lg-12">
                    <div class="well bs-component">

                        <hr />
                        <dl class="dl-horizontal">
                            <div class="form-group col-lg-12">
                                <label class="col-lg-4 control-label">
                                    @Html.DisplayNameFor(model => model.SessionName)
                                </label>
                                <div class="col-md-8">
                                    @Html.DisplayFor(model => model.SessionName)
                                </div>
                            </div>
                            <div class="form-group col-lg-12">
                                <label class="col-lg-4 control-label">
                                    @Html.DisplayNameFor(model => model.ClassName)
                                </label>
                                <div class="col-md-8">
                                    @Html.DisplayFor(model => model.ClassName)
                                </div>
                            </div>
                            <div class="form-group col-lg-12">
                                <label class="col-lg-4 control-label">
                                    @Html.DisplayNameFor(model => model.TotalFee)
                                </label>
                                <div class="col-md-8">
                                    @Html.DisplayFor(model => model.TotalFee)
                                </div>
                            </div>

                    </div>

                    <p>
                        @Html.ActionLink("Edit", "Create", new { id = Model.FeeMasterId }) |
                        @Html.ActionLink("Back to List", "Index")
                    </p>
                </div>
            </div>

        </div>
    </div>
</div>
    </div>


<!-- Javascripts-->
<script src="~/js/jquery-2.1.4.min.js"></script>
<script type="text/javascript" src="~/js/plugins/bootstrap-datepicker.min.js"></script>
<script type="text/javascript" src="~/js/plugins/select2.min.js"></script>
<script type="text/javascript" src="~/js/plugins/bootstrap-datepicker.min.js"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $(window).load(function () {
            $('#loader').hide();
        });
    });
    $('#demoDate').datepicker({
        format: "yyyy-mm-dd",
        autoclose: true,
        todayHighlight: true
    });
    $('#DateOfBirth').datepicker({
        format: "yyyy-mm-dd",
        autoclose: true,
        todayHighlight: true
    });
    $('#DateOfJoining').datepicker({
        format: "yyyy-mm-dd",
        autoclose: true,
        todayHighlight: true
    });
</script>



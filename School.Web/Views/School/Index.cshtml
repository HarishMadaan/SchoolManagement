@{
    ViewBag.Title = "School";
    Layout = "~/Views/Shared/_LayoutMaster.cshtml";
}

@section css{

    <link href="~/css/site-examples.css" rel="stylesheet" />
    <link href="~/css/dataTables.css" rel="stylesheet" type="text/css">
    <link href="~/css/responsive.css" rel="stylesheet" type="text/css">
    <link href="~/css/table.css" rel="stylesheet" type="text/css">
    <link href="~/css/font-awesome.css" rel="stylesheet" />

    <style type="text/css">
        .filteredComment {
            color: #253340;
            font-weight: bold;
        }
    </style>
}


<div class="row  border-bottom white-bg dashboard-header">
    <ol class="breadcrumb">
        <li><a href="dashboard.htm"><i aria-hidden="true" class="fa fa-home"></i> </a></li>
        <li class="active">School Management</li>
    </ol>
</div>
<div class="row">
    <div class="col-lg-12">
        <div class="wrapper wrapper-content">
            <div data-ng-show="ShowFarmerRegistrationListing">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="panel panel-default PanelBord-second">

                            <div class="panel-heading"><span class="DetGate">School Management Listing</span>   <span class="GateBtn"><button ng-click="FarmerRegistrationManagement.ShowHide()" data-ng-app="App" class="btn btn-primary BtnBack">Add New Farmer <span style="color:#fff; margin:0;" class="glyphicon glyphicon-plus-sign" aria-hidden="true"></span></button></span></div>

                            <div class="panel-body">
                                <div class="row">
                                    <div class="col-md-12 align-right">
                                        <div class="pull-right">
                                            <div class="form-group col-md-4">
                                            </div>
                                            <div class="form-group col-md-8">
                                                <input type="text" id="txtSearch" name="txtSearch" ng-model="SearchText" class="form-control input-sm" style="display:inline;width:auto;" placeholder="Search Farmers" size="40" /><button ng-click="FarmerRegistrationManagement.GetSearchFarmerDetails()"><i class="glyphicon glyphicon-search "></i></button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-12">


                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
            <div data-ng-hide="ShowFarmerRegistrationListing">
            </div>

        </div>
    </div>
</div>
